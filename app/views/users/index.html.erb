<h1>userの一覧ページ</h1>

<%= render 'users/list', user: @user, book: @book, books: @books %>

<h1>Users</h1>

<h5>image</h5>
<h5>name</h5>

<% @users.each do |user| %>
    <%= image_tag @user.get_profile_image %>
        <% if @users.present? %> 
        <p><strong><%= @user.name %></strong></p>
        <% end %>
        <%= link_to 'show', user_path(user) %>
    <% end %>
